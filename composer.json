{
    "name": "icebox/framework",
    "type": "library",
    "description": "The Icebox Framework.",
    "keywords": ["framework", "icebox"],
    "homepage": "http://torovin.com/icebox",
    "require": {
        "icebox/activerecord": "@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "6.5"
    },
    "autoload": {
        "files": [
            "src/helper.php"
        ],
        "psr-4": {
          "Icebox\\": "src/icebox/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Icebox\\Tests\\": "tests/"
        }
    }
}
